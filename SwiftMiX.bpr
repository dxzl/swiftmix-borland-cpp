PROJECT = SwiftMiX.exe
OBJFILES = OBJ\MyCheckLst.obj OBJ\SwiftMiX.obj OBJ\Main.obj OBJ\About.obj OBJ\SMList.obj \
  OBJ\Progress.obj OBJ\AutoSize.obj OBJ\FormExportMode.obj OBJ\DefaultStrings.obj \
  OBJ\XmlTable.obj OBJ\FormImportMode.obj OBJ\RegHelper.obj OBJ\FormSFDlg.obj \
  OBJ\FormDirDlg.obj OBJ\FormImport.obj OBJ\FormExport.obj OBJ\FormOFMSDlg.obj
RESFILES = SwiftMiX.res OBJ\SwiftMiX.res
RESDEPEN = $(RESFILES) Main.dfm About.dfm SMList.dfm Progress.dfm AutoSize.dfm \
  FormExportMode.dfm FormImportMode.dfm FormSFDlg.dfm FormDirDlg.dfm \
  FormImport.dfm FormExport.dfm
LIBFILES =
LIBRARIES = vcldb40.lib ibsmp40.lib \
  vcljpg40.lib dclusr40.lib dclocx40.lib Vclx40.lib Vcl40.lib
SPARELIBS = Vcl40.lib Vclx40.lib dclocx40.lib dclusr40.lib vcljpg40.lib ibsmp40.lib \
  vcldb40.lib
PACKAGES = Vcl40.bpi Vclx40.bpi vcljpg40.bpi
DEFFILE =
# ---------------------------------------------------------------------------
PATHCPP = .;
PATHASM = .;
PATHPAS = .;
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
USERDEFINES =
SYSDEFINES = NO_STRICT
# ---------------------------------------------------------------------------
CFLAG1 = -I$(BCB)\bin;$(BCB)\Imports;SwiftMiX;$(BCB)\include;$(BCB)\include\vcl -O2 -Hc \
  -H=$(BCB)\lib\vcl40.csm -w -Ve -a8 -5 -k- -vi -c -b- -w-par -w-inl -Vx -tW -tWM \
  -D$(SYSDEFINES);$(USERDEFINES)
PFLAGS = -U$(BCB)\bin;$(BCB)\Imports;SwiftMiX;$(BCB)\Projects\Lib;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -I$(BCB)\bin;$(BCB)\Imports;SwiftMiX;$(BCB)\include;$(BCB)\include\vcl -N2OBJ \
  -N0OBJ -$Y- -$L- -$D- -v -JPHNE -M
RFLAGS = -i$(BCB)\bin;$(BCB)\Imports;SwiftMiX;$(BCB)\include;$(BCB)\include\vcl
AFLAGS = /i$(BCB)\bin /i$(BCB)\Imports /iSwiftMiX /i$(BCB)\include /i$(BCB)\include\vcl \
  /mx /w2 /zn
LFLAGS = -L$(BCB)\bin;$(BCB)\Imports;SwiftMiX;$(BCB)\Projects\Lib;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -aa -Tpe -x -Gn -w
# ---------------------------------------------------------------------------
ALLOBJ = c0w32.obj sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mt.lib
